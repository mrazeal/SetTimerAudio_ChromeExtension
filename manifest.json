{
    "manifest_version": 2,
    "name": "Set audio for turn end",
    "description": "Replaces audio that is played for end of turns.",
    "version": "0.2",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
}